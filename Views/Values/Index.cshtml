@model IEnumerable<MIS4200_Team10.Models.Values>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.coreValue)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.coreValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.valueID }) |
            @Html.ActionLink("Details", "Details", new { id=item.valueID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.valueID })
        </td>
    </tr>
}

</table>
